package ensyu;

public class Ensyu22 {
	public static void main(String[] args) {
		for (int n = 10000; n <= 10100; n++) {
			priNum(n);
		}
	}

	public static int priNum(int num) {
		int i;
		for (i = 2; i <= (num / 2); i++) {
			if (num % i == 0) {
				break;
			}
		}
		if (i > (num / 2))
			return num;
		return i;
	}
}
