package ensyu;

public class Ensyu28 {
	public static void main(String[] args) {
		int array[] = new int[7];
		for (int i = 0; i < 7; i++) {
			array[i] = new java.util.Random().nextInt(10) + 1;
			System.out.print(array[i] + " ");
		}
		for (int o = 0; o < 7; o++) {
			while (array[o] == o) {
				System.out.print("*");
			}
		}
	}
}
