package ensyu;

public class Ensyu8 {
	public static void main(String[] args) {
		Dog myDog = new Dog();
		myDog.SetName("name");
		myDog.ShowProfile();
	}
	class Dog {
		private String Name;
		public void SetName( String nm ) {
			Name = nm;
		}
		public void ShowProfile(){
			System.out.println( "名前は、" + Name + "です。" );
		}
	}
}


