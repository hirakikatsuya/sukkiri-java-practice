package ensyu;

public class Ensyu32 {
	public static void main(String[] args) {
		int a = 0;
		int b = 0;
		int c = 0;
		while (a < 30) {
			if (a == 0) {
				System.out.println(a);
				a++;
			}else if (a == 1) {
				System.out.println(a);
			}
			System.out.println(a);
			b+=a;
			System.out.println(b);
			c+=b;
			System.out.println(c);
		}
	}
}
