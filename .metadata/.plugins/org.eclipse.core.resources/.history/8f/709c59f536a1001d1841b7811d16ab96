package ensyu;

public class Ensyu16 {
	public static void main(String[] args) {
		int List[] = new int[10];
		try (java.util.Scanner scanner = new java.util.Scanner(System.in)) {
			for (int i = 0; i < 10; i++) {
				List[i]=scanner.nextInt();
			}
			for(int o = 0; o<10;o++) {
				System.out.println(List[o]*2);
			}
		}
	}
}
