package practice;

public class Practice5_1 {
	public static void introduceOneself() {
		String name = "佐藤";
		int age = 20;
		float length = 180.5F;
		char eto = '寅';
		System.out.println("私の名前は"+name+"、年齢は"+age+"、身長は"+length+"、干支は"+eto);
	}

	public static void main(String[] args) {
		introductionOneself;
	}
}
